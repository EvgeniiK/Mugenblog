<p id="notice"><%= notice %></p>
<head>
<h1>MugenBlog</h1>
<% if current_user.present? %>
  <h3><%= "Dont forget your username: #{current_user.username}" %></h3>
  <h6>just to let you know</h6>
  <%= link_to 'sign_out', destroy_user_session_path, method: :delete %>
<% else %>
  <%= link_to 'sign_in', user_session_path %>
  <%= link_to 'register', new_user_registration_path %>
<% end %>

<br>
</head>
<body>
<h2>Some unsorted information</h2>

<table>
  <thead>
    <tr>
      <th>Theme</th>
      <th>Command</th>
      <th>Description</th>
      <th>Link</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <div style="width: 1000px">
      <% @raw_data.each do |raw_datum| %>
        <tr>
          <td><%= raw_datum.theme %></td>
          <td><%= raw_datum.command %></td>
          <td><%= raw_datum.description %></td>
          <td><%= link_to raw_datum.link, raw_datum.link %></td>
          <td><%= raw_datum.try(:user).try(:username) || 'anon' %></td>
          <td><%= link_to 'Show', raw_datum %></td>
          <td><%= link_to 'Edit', edit_raw_datum_path(raw_datum) %></td>
        </tr>
      <% end %>
    </div>
  </tbody>
</table>

<br>

<h2>New information</h2>

<%= render 'form' %>
</body>
<footer>капинотрайт <%= Time.now.year %></footer>
